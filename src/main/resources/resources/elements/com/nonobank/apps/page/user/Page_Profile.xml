	<!-- 支付密码确认密码 -->
	<WebInput name="newZFPassword" xpath="//input[@id='newZFPassword']">
	</WebInput>
	<!-- 支付密码修改确认 -->
	<WebInput name="setPasswordbtn" xpath="//input[@id='setPasswordbtn']">
	</WebInput>
</elements>