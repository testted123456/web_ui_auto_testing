	<WebCommon name="passwordSetting"
		xpath="//li[contains(@onclick,'/User/ChangePassword/login')]">
	</WebCommon>
	<!-- 支付密码设置 -->
	<WebCommon name="payPasswordSetting"
		xpath="//li[contains(@onclick,'/User/ChangePassword/pay')]">
	</WebCommon>
	<!-- 支付密码新密码 -->
	<WebInput name="newZFPwd" xpath="//input[@id='newZFPwd']">
